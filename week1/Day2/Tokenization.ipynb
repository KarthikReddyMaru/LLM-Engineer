   ],
   "source": [
    "from tiktoken import encoding_for_model\n",
    "\n",
    "tokenizer = encoding_for_model(model_name = 'gpt-4.1-mini')\n",
    "\n",
    "text = \"Hi, My name is Karthik!\"\n",
    "\n",
    "tokens = tokenizer.encode(text = text)\n",
    "\n",
    "for token in tokens:\n",
    "    print(f\"{tokenizer.decode(tokens = [token])} = {token}\")"
   ]
  }
